#: usr/lib/linuxmint/mintupgrade/checks.py:473
msgid "Package"
msgstr "بسته"

#: usr/lib/linuxmint/mintupgrade/checks.py:277
msgid "Operation"
msgstr "عملیات"

#: usr/lib/linuxmint/mintupgrade/checks.py:279
